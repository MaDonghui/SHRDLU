    <sort name="parser-function" super="grammar-concept"/>   
    <sort name="#derefFromContext" super="parser-function"/>   <!-- This function tries to find which object from the natural language context are referred to by a given clause -->
    <sort name="#derefUniversal" super="parser-function"/>  <!-- This function generates a term that represents all the entities represented by the clause (e.g., in the sentence "all men are mortal", the noun phrase "all men" does not have a referent in the context, so it's a universal) --> 
    <sort name="#derefHypothetical" super="parser-function"/>  <!-- This function dereferences a hypothetical entity (usually referred to by name), for example, in "Socrates was a man", the first time "Socrates" is mentioned, it it a hypothetical person, since it's not in the context. --> 
    <sort name="#derefQuery" super="parser-function"/>  <!-- This function dereferences a query, for example in "what is your name?", the "your name" part is a query name(X,Y), where "X" is the dereference of "you" and "Y" is the query variable. --> 
    <sort name="#symbolToSort" super="parser-function"/>
    <sort name="#concatenateSymbols" super="parser-function"/>
    <sort name="#subsumes" super="parser-function"/>
    <sort name="#completeVerbArgumentsFromContext" super="parser-function"/>
    <sort name="#token" super="parser-function"/>  <!-- #token('hello') is equivalent to 'hello', 
                                                        just so that we can match with a constant in a variable -->

